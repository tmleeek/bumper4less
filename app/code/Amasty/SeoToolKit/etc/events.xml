<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">
    <event name="controller_action_predispatch">
        <observer name="amasty_seo_toolkit" instance="Amasty\SeoToolKit\Observer\Redirect" />
    </event>
    <event name="controller_front_send_response_before">
        <observer name="amasty_seo_toolkit_controller_front_send_response_before"
                  instance="Amasty\SeoToolKit\Observer\MoveContentToHead"
        />
    </event>
</config>
